<h1>All Tweets</h1>
<%= link_to 'New Tweet', new_post_path %>

<% @posts.each do |post| %>
  <div>
    <p><%= post.content %></p>
    <%= link_to 'Show', post %>
    <%= link_to 'Edit', edit_post_path(post) %>
    <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
<% end %>